{
    "swagger": "2.0",
    "info": {
        "title": "SEMARD Water Quality API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/get": {
            "get": {
                "tags": [
                    "Get all registers"
                ],
                "summary": "API GET",
                "description": "Se obtienen todos los registros de la base de datos",
                "operationId": "getAll",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/nodo"
                        }
                    },
                    "400": {
                        "description": "Server error"
                    },
                    "404": {
                        "description": "Access not found"
                    }
                }
            }
        }
    },
    "definitions": {
        "nodo": {
            "type": "object",
            "properties": {
                "id_node": {
                    "type": "integer",
                    "example": 1
                },
                "description": {
                    "type": "string",
                    "example": "Laguito"
                },
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/data"
                    }
                }
            }
        },
        "data": {
            "type": "object",
            "properties": {
                "id_registro": {
                    "type": "integer",
                    "example": 15
                },
                "sensors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/sensor"
                    }
                },
                "timestamp": {
                    "type": "string",
                    "example": "2021-05-20 12:00:00"
                }
            }
        },
        "sensor": {
            "type": "object",
            "properties": {
                "ec": {
                    "type": "integer",
                    "example": 15
                },
                "od": {
                    "type": "float",
                    "example": 7.5
                },
                "ph": {
                    "type": "float",
                    "example": 25.5
                },
                "tds": {
                    "type": "float",
                    "example": 25.5
                },
                "temp": {
                    "type": "float",
                    "example": 25.5
                }
            }
        }
    }
}